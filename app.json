{
  "expo": {
    "name": "voicebotsai",
    "slug": "voicebotsai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "voicebotsai",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "ignoreNodeModulesValidation": ["#current-cloud-backend"]
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["RECORD_AUDIO", "android.permission.RECORD_AUDIO"],
      "blockedPermissions": [],
      "package": "com.voicebotsai"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-voice/voice"],
    "extra": {
      "eas": {
        "projectId": "5896acb7-11d8-45f0-a509-89453ad0cf21"
      }
    }
  }
}
